{
  "name": "ai-interview-backend",
  "version": "1.0.0",
  "description": "AI Interview Web Application Backend with OpenAI and ElevenLabs integration",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm install && prisma generate && tsc",
    "start": "node dist/index.js",
    "postinstall": "prisma generate",
    "dev": "nodemon --exec ts-node src/index.ts",
    "dev:watch": "nodemon --watch 'src/**/*.ts' --exec ts-node src/index.ts",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:reset": "prisma migrate reset",
    "db:studio": "prisma studio",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "ai",
    "interview",
    "openai",
    "elevenlabs",
    "resume"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@elevenlabs/elevenlabs-js": "^2.8.0",
    "@prisma/client": "^6.13.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/pdf-parse": "^1.1.5",
    "@types/uuid": "^10.0.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "docx": "^9.5.1",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mammoth": "^1.10.0",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "openai": "^5.12.0",
    "pdf-parse": "^1.1.1",
    "prisma": "^6.13.0",
    "sqlite3": "^5.1.7",
    "typescript": "^5.9.2",
    "uuid": "^11.1.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/node": "^24.2.0",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2"
  }
}
